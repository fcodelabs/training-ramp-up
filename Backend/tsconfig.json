{
  "compilerOptions": {
    /*Language and Environment*/
    "target": "ES6",

    /* Modules */
    "module": "commonjs",
    //"rootDirs": [],

    /* Interop Constraints */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,

    /* Completeness */
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true

    /* Emit */
    //"outFile": "./",
    //"outDir": "./",
  }
}
